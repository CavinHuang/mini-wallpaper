{
  "name": "api-server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "pm2": "pm2 start ecosystem.json",
    "build": "cross-env NODE_ENV=production tsc --build && cpx \"src/**/*.{js,json}\" dist",
    "start2": "node ./dist/app.js",
    "watch": "cross-env NODE_ENV=production nodemon --ext js,ts --exec yarn start",
    "start": "cross-env NODE_ENV=development nodemon --ext js,ts --exec ts-node -r tsconfig-paths/register ./src/app.ts",
    "dev": "cross-env NODE_ENV=development nodemon --watch ./src/* -e ts --exec ts-node -r tsconfig-paths/register ./src/app.ts",
    "clear:js": "rimraf ./src/**/*.js",
    "plop": "plop",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:create": "cross-var npm run typeorm -- migration:create ./src/models/migration/$npm_config_name",
    "migration:run": "npm run typeorm -- migration:run -d ormconfig.ts",
    "migration:generate": "cross-var npm run typeorm -- migration:generate src/models/migration -d ormconfig.ts -p --dr --ch",
    "generate:entity": "npx typeorm-model-generator -h localhost -d test1 -p 3306 -u test1 -x cMne3SPiXjnjAr8W -e mysql -o entities --noConfig true --ce pascal"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@koa/cors": "^3.3.0",
    "axios": "^1.1.3",
    "bcrypt": "^5.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "deepmerge": "^4.2.2",
    "dotenv": "^16.0.1",
    "fs-readdir-recursive": "^1.1.0",
    "glob": "^8.0.3",
    "ioredis": "^5.2.2",
    "joi": "^17.6.0",
    "jsonwebtoken": "^8.5.1",
    "koa": "^2.13.4",
    "koa-body": "^5.0.0",
    "koa-helmet": "^6.1.0",
    "koa-jwt": "^4.0.3",
    "koa-router": "^10.1.1",
    "koa-static": "^5.0.0",
    "koa2-request": "^1.0.4",
    "log-update": "^4.0.0",
    "log4js": "^6.3.0",
    "md5": "^2.3.0",
    "moment": "^2.29.3",
    "mysql2": "^2.3.3",
    "node-cache": "^5.1.2",
    "qiniu": "^7.7.0",
    "qs": "^6.10.3",
    "redis": "^3.1.1",
    "reflect-metadata": "^0.1.13",
    "streamroller": "^3.1.1",
    "typeorm": "^0.3.11"
  },
  "devDependencies": {
    "@types/koa": "^2.13.5",
    "@types/node": "*",
    "chalk": "^4.1.2",
    "cross-env": "^7.0.3",
    "cross-var": "^1.1.0",
    "nodemon": "^2.0.16",
    "plop": "^3.1.1",
    "rimraf": "^3.0.2",
    "ts-node": "^10.7.0",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.7.3"
  }
}
