"pre1111typeorm": "rimraf ormconfig.json && ts-node -r tsconfig-paths/register initOrmConfig.ts",
"typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli -d ormconfig.ts",
"migration:create": "cross-var ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli -- migration:create ./src/models/migration/$npm_config_name",
"migration:generate": "npm run typeorm -- migration:generate ./src/models/migration/$npm_config_name",
"migration:run": "npm run typeorm -- migration:run"


