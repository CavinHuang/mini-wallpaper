/* 商品列表 */
.pic-list{
		display:-webkit-box;
		display:-webkit-flex;
		display:flex;
		-webkit-flex-wrap:wrap;
		        flex-wrap:wrap;
		padding: 0 30rpx;
		background: #fff;
}
.pic-item{
		display:-webkit-box;
		display:-webkit-flex;
		display:flex;
		-webkit-box-orient: vertical;
		-webkit-box-direction: normal;
		-webkit-flex-direction: column;
		        flex-direction: column;
		width: 32%;
		margin-bottom: 20rpx;
}

