
.order-time-line{
	padding-left: 0rpx;
}
.order-time-main{
	width: 100%;
	min-height: 100vh;
}
.order-list-item-tit{
	position: relative;
	height: 60rpx;
	line-height: 60rpx;
}
.order-list-item-tit .order-type{
	width: 60rpx;
	height: 60rpx;
	position: absolute;
	left: -90rpx;
	top: 0;
}
.order-list-item-l{
	width: 200rpx;
	height: 150rpx;
	border-radius: 8rpx;
	background: #F2F2F2;
}
.order-list-item-r{
	width: calc(100% - 200rpx);
	padding-left: 30rpx;
}
.pb24 {
    padding-bottom: 40rpx;
}

